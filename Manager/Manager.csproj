  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="assets\**" />
    <EmbeddedResource Remove="assets\**" />
    <None Remove="assets\**" />
  </ItemGroup>

  <Import Project="..\Sdks\PluginSdk\PluginSdk.projitems" Label="Shared" />

  <Import Project="..\Sdks\GameSdk\GameSdk.projitems" Label="Shared" />

  <Import Project="..\Core\Core.projitems" Label="Shared" />
</Project>